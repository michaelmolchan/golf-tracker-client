{{#each rounds as |round|}}
  <ul data-id="{{round.id}}">
    <li><h1 data-title='{{round.course}}'> <small>Course: {{round.course}}</small></h1></li>
    <li>{{round.date}}</li>
    <li>{{round.location}}</li>
    <li>{{round.par}}</li>
    <li>{{round.score}}</li>
    <li><button class="btn btn-info update-round" data-taz="hello taz" data-id='{{round.id}}'>Update Round</button></li>
    <li><button class="btn btn-danger remove-round" data-taz="hello taz" data-id='{{round.id}}'>Remove Round</button></li>
  </ul>
{{/each}}
